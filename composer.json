{
  "name": "mobilozophy/laravel-suspender",
  "description": "Suspends and adds cascading soft deletes/suspensions to Eloquent models.",
  "license": "MIT",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Jeff Wray",
      "email": "jwray@mobilozophy.com",
      "homepage": "https://www.mobilozophy.com"
    }
  ],
  "require": {
    "php": "^7.2.5",
    "illuminate/database": "^6.0|^7.0",
    "illuminate/events": "^6.0|^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "mobilozophy\\Database\\Support\\": "src/"
    }
